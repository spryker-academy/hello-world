{
    "name": "spryker-academy/hello-world",
    "description": "Spryker Academy HelloWorld Training Module - Learn Spryker fundamentals through progressive exercises",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Spryker Academy",
            "email": "academy@spryker.com"
        }
    ],
    "keywords": [
        "spryker",
        "training",
        "tutorial",
        "hello-world",
        "learning"
    ],
    "require": {
        "php": ">=8.4",
        "spryker/kernel": "^3.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/gui": "^3.0.0 || ^4.0.0",
        "spryker/propel-orm": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerAcademy\\": "src/SprykerAcademy/",
            "Orm\\": "src/Orm/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerAcademyTest\\": "tests/SprykerAcademyTest/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-ilt/202512.0/basics/hello-world-back-office/skeleton": "1.0-dev",
            "dev-ilt/202512.0/basics/hello-world-back-office/complete": "1.0.x-dev",
            "dev-ilt/202512.0/basics/data-transfer-object/skeleton": "2.0-dev",
            "dev-ilt/202512.0/basics/data-transfer-object/complete": "2.0.x-dev",
            "dev-ilt/202512.0/basics/message-table-schema/skeleton": "3.0-dev",
            "dev-ilt/202512.0/basics/message-table-schema/complete": "3.0.x-dev",
            "dev-ilt/202512.0/basics/module-layers/skeleton": "4.0-dev",
            "dev-ilt/202512.0/basics/module-layers/complete": "4.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
